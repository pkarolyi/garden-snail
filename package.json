{
  "name": "garden-snail",
  "version": "1.3.0",
  "description": "Turborepo remote cache server",
  "author": "Peter Karolyi <peter@karolyi.dev>",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "test:debug": "vitest --inspect-brk --inspect --logHeapUsage --threads=false",
    "test:e2e": "vitest run -c ./test/vitest.config.e2e.ts",
    "test:e2e:watch": "vitest -c ./test/vitest.config.e2e.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.782.0",
    "@aws-sdk/lib-storage": "3.782.0",
    "@nestjs/common": "11.0.13",
    "@nestjs/config": "4.0.2",
    "@nestjs/core": "11.0.13",
    "@nestjs/platform-fastify": "11.0.13",
    "fastify": "5.2.2",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.2",
    "zod": "3.24.2"
  },
  "devDependencies": {
    "@nestjs/cli": "11.0.6",
    "@nestjs/schematics": "11.0.3",
    "@nestjs/testing": "11.0.13",
    "@swc/cli": "0.6.0",
    "@swc/core": "1.11.18",
    "@types/node": "20.17.30",
    "@types/supertest": "6.0.3",
    "@typescript-eslint/eslint-plugin": "8.29.0",
    "@typescript-eslint/parser": "8.29.0",
    "@vitest/coverage-v8": "3.1.1",
    "eslint": "8.57.1",
    "eslint-config-prettier": "10.1.1",
    "eslint-plugin-prettier": "5.2.6",
    "prettier": "3.5.3",
    "prettier-plugin-organize-imports": "4.1.0",
    "source-map-support": "0.5.21",
    "ts-loader": "9.5.2",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.8.3",
    "unplugin-swc": "1.5.1",
    "vitest": "3.1.1"
  },
  "packageManager": "pnpm@9.5.0",
  "engines": {
    "node": "20.18.0"
  }
}
